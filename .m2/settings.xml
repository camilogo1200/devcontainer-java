<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd"
          xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <servers>
    <server>
      <id>ghixbuild</id>
      <username>devuser</username>
      <password>Getinsured@1234</password>
    </server>
  </servers>
  <mirrors>
    <mirror>
      <id>maven-default-http-blocker</id>
      <mirrorOf>dummy</mirrorOf>
      <name>Dummy mirror to override default blocking mirror that blocks http</name>
      <url>http://0.0.0.0/</url>
    </mirror>
    <mirror>
      <id>jboss-public-repository-group-https</id>
      <mirrorOf>jboss-public-repository-group</mirrorOf>
      <name>Jboss public https</name>
      <url>https://repository.jboss.org/nexus/content/groups/public/</url>
    </mirror>
  </mirrors>
  <profiles>
    <profile>
      <id>artifactory</id>
      <repositories>
        <repository>
          <id>snapshots</id>
          <name>libs-snapshot</name>
          <snapshots/>
          <url>http://artifactory-ii.eng.vimo.com:8081/artifactory/libs-snapshot</url>
        </repository>
        <repository>
          <id>ghixbuild</id>
          <name>libs-release</name>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <url>http://artifactory-ii.eng.vimo.com:8081/artifactory/libs-release</url>
        </repository>
        <repository>
          <id>remote</id>
          <name>remote-repos</name>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <url>http://artifactory-ii.eng.vimo.com:8081/artifactory/remote-repos</url>
        </repository>
        <repository>
          <id>central-maven-repo</id>
          <name>central-maven-repo-https</name>
          <url>https://repo1.maven.org/maven2</url>
        </repository>
      </repositories>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>artifactory</activeProfile>
  </activeProfiles>
</settings>
